{
  "name": "epm-live-automation",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "webdriver-manager update && npm run clean",
    "lint": "tslint \"e2e/**/*.ts\" --fix",
    "clean": "rm -rf auto-generated/allure-results *.log",
    "test": "protractor protractor.conf.js",
    "e2e": "protractor protractor.conf.js",
    "e2e:bs": "protractor protractor.browserstack.conf.js",
    "e2e:bsf": "protractor protractor.browserstack-serial.conf.js",
    "e2e-selenium": "protractor protractor.selenium-hub.conf.js",
    "e2e:report": "allure generate auto-generated/allure-results -o e2e-report --clean",
    "e2e-tc": "protractor protractor.local.conf.js",
    "build": "tsc -p ./"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "tslint \"e2e/**/*.ts\" --fix",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/compiler": "^6.1.8",
    "@angular/core": "^6.1.8",
    "@aurea/aurea-jasmine-testrail-reporter": "^1.0.3",
    "@aurea/protractor-automation-helper": "0.0.1",
    "allure-commandline": "^2.5.0",
    "browserstack-local": "^1.3.0",
    "jasmine-allure-reporter": "^1.0.2",
    "jasmine-reporters": "^2.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "log4js": "^3.0.5",
    "moment": "^2.22.2",
    "node-vibrant": "^3.0.0",
    "protractor": "~5.4.1",
    "request-promise-native": "^1.0.5",
    "rxjs": "^6.3.2",
    "shortid": "^2.2.8",
    "testrail-api": "^1.2.0",
    "testrail-promise": "^0.1.18",
    "underscore": "^1.8.3",
    "unirest": "^0.5.1"
  },
  "devDependencies": {
    "@types/jasmine": "~2.8.7",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~10.10.1",
    "codelyzer": "~4.4.4",
    "husky": "^1.0.0-rc.14",
    "jasmine-core": "~3.2.1",
    "lint-staged": "^7.3.0",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.0.3"
  }
}
