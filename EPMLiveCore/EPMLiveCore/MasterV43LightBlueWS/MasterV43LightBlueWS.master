<script type="text/javascript">

    if ($('#ctl00_PlaceHolderSearchArea_ctl01_S3031AEBB_InputKeywords').length == 0) {
        var searchId = 'idSearchString';
    }
    else {
        var searchId = 'ctl00_PlaceHolderSearchArea_ctl01_S3031AEBB_InputKeywords';
    }

    var searchIdjq = "#" + searchId

    $(searchIdjq).addClass('jq_watermark');

    $(searchIdjq).attr('alt', 'Search...');
    $(searchIdjq).attr('title', 'Search...');
    $(searchIdjq).removeAttr("value");
    $(searchIdjq).removeAttr("onblur");

    $(searchIdjq).blur(function () {
        $("#searchDiv").fadeOut('fast', function () {
            $(searchIdjq).val('');
        });
    });


    if ($(searchIdjq).length != 1) {
        if (searchId != 'idSearchString') {
            document.getElementById('searchicon').style.display = "none";
        }
    }

    $(document).click(function () {
        if ($('#ctl00_PlaceHolderSearchArea_SearchString').length == 0) {
            $("#searchDiv").fadeOut('fast', function () {
                $(searchIdjq).val('');
            });
        }
    });

    $("#searchDiv").click(function (event) {
        event.stopPropagation();
    });

    $("#searchicon").click(function (event) {
        event.stopPropagation();
        toggleSearch('searchicon', searchId);
    });

    if ($('#ctl00_PlaceHolderSearchArea_SearchString').length != 0) {
        objDiv = document.getElementById('searchDiv');
        objDiv.style.position = "relative";
        objDiv.style.display = "inline";
        objDiv.style.cssFloat = "left";
        objDiv.style.paddingTop = "10px";
        objDiv.style.paddingRight = "10px";
        document.getElementById('ctl00_PlaceHolderSearchArea_SearchString').style.width = "175px";

        $('#searchDiv').fadeIn('fast', function () {
            $('#ctl00_PlaceHolderSearchArea_SearchString').focus();
        });
        objIcon = document.getElementById('searchicon');
        objNotch = document.getElementById('searchNotch');
        objIcon.style.display = "none";
        objNotch.style.display = "none";
    }
    else {
        $(window).resize(function () { document.getElementById('searchDiv').style.display = "none"; });
    }

    if ($('#ctl00_PlaceHolderMain_ctl01_SearchBox2_SFCD52D7A_InputKeywords').length != 0) {
        document.getElementById('searchicon').style.display = "none";
    }
	
</script>


