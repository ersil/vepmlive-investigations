<script type="text/javascript">

    function toggleSearch(id, searchId) {

        obj = document.getElementById(id);
        objSearchDiv = document.getElementById('searchDiv');
        objNotch = document.getElementById('searchNotch');

        objInput = document.getElementById(searchId);

        var searchId = "#" + searchId;
        $(searchId).addClass('jq_watermark');

        //var pos = findPos(obj);

        //posLeft = pos[0] - 210;
        //posTop = pos[1] - 4;

        if (objSearchDiv.style.display == "none") {
            //objSearchDiv.style.left = posLeft + 'px';
            //objSearchDiv.style.top = posTop + 'px';
            $(objSearchDiv).fadeIn('fast', function () {
                $(searchId).focus();
            });
        }
        else {
            //objSearchDiv.style.left = posLeft;
            //objSearchDiv.style.top = posTop;
            $(objSearchDiv).fadeOut('fast', function () {
                $(searchId).val('');
            });
        }

    }

    function findPos(obj) {
        var curleft = curtop = 0;
        if (obj.offsetParent) {
            curleft = obj.offsetLeft
            curtop = obj.offsetTop
            while (obj = obj.offsetParent) {
                curleft += obj.offsetLeft
                curtop += obj.offsetTop
            }
        }
        return [curleft, curtop];
    }


</script>
