<body style="margin: 0px; padding: 0px;">
    <%
       
        if (HttpContext.Current.Session[sitecollectionid +"Role"] != null && HttpContext.Current.Session[sitecollectionid+"Role"].ToString() != "Report Writers")
        {%>
    <style>
        .designer-only {
            display: none !important;
        }

        .thumb-buttons {
            display: none !important;
        }
    </style>
    <%}%>
    
        <%if (HttpContext.Current.Session["InFrame"] == "1")
        { %> 
    <style>
         .header
        {
            display: none;
        }
         </style>   
        <%
        }
        %>
    

    <div class="layout">
        <div class="header applyStyles" id="whiteHeader" style="display: none !important">
            <div class="page" style="font-family: Segoe UI,Tahoma,Verdana,Arial,Helvetica,sans-serif">
                <div class="left-logo">
                    <img src="rs.aspx?image=ModernImages.bi-logo.png" alt="Business intelligence" />
                </div>
                <div class="right-logo">
                    <a href="http://izenda.com" style="position: relative; top: 4px;">
                        <img runat="server" id="rightLogo" src="rs.aspx?image=ModernImages.izenda-logo-4.gif" alt="Izenda Reports" /></a>
                </div>
                <div class="clearfix"></div>
            </div>
        </div>

        <script type="text/javascript">
		function insertParam(key, value) {
		  key = escape(key); value = escape(value);
		  var kvp = document.location.search.substr(1).split('&');
		  if (kvp.length == 1 && kvp[0] == '')
			  kvp.length = 0;
		  var i = kvp.length; var x; while (i--) {
			x = kvp[i].split('=');
			if (x[0] == key) {
			  x[1] = value;
			  kvp[i] = x.join('=');
			  break;
			}
		  }
		  if (i < 0) { kvp[kvp.length] = [key, value].join('='); }
		  document.location.search = kvp.join('&');
		}

		function ShowRlToDbLoading() {
		  document.getElementById('mainContentDiv').style.display = 'none';
		  var dld = document.getElementById('dashLoadingVg');
		  dld.style.height = document.body.scrollHeight + 'px';
		  dld.style.display = '';		  
		  var limg = document.getElementById('limgDb');
		  var lw = document.getElementById('loadingWordDb');
		  var vSize = document.body.offsetHeight;
		  lw.style.paddingTop = (3 + vSize / 3) + 'px';
		}



        </script>


        <div class="top-nav applyStyles" id="blueHeader">
            <div class="page">
                <ul id="topnav" style="margin: 0px; padding: 0px;">
                    <li class="top-nav-item"><a href="ReportList.aspx?sitecollectionid=<%=sitecollectionid%>">Reports</a></li>
                    <li class="top-nav-item"><a href="Dashboards.aspx?clear=1&sitecollectionid=<%=sitecollectionid%>" onclick="ShowRlToDbLoading();">Dashboards</a></li>
                    <li class="top-nav-item btn-group">
                        <a class="btn" href="ReportDesigner.aspx?clear=1&tab=Data+Sources&sitecollectionid=<%= sitecollectionid%>"><span class="plus">+</span> New</a>
                        <button type="button" class="btn dropdown-toggle" data-toggle="dropdown">
                            <span class="caret"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="ReportDesigner.aspx?clear=1&tab=Data+Sources&sitecollectionid=<%=sitecollectionid%>">Report</a></li>
                            <li runat="server" id="irItem"><a href="InstantReport.aspx?sitecollectionid=<%=sitecollectionid%>">Instant Report</a></li>
                            <li><a href="DashboardDesigner.aspx?clear=1&sitecollectionid=<%=sitecollectionid%>">Dashboard</a></li>
                        </ul>
                    </li>



