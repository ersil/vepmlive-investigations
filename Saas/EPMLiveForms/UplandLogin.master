	}
}

@media screen and (max-width: 480px) {
	#login-ad-main {
		display:none;
	}
	#login-wrapper {
		width:300px;
		top:0px;
	}
	#login-main {
		border-radius: 3px 3px 3px 3px;
		width:300px;
	}
	.input-img {
		display:none;
	}
	input[type="text"], input[type="email"], input[type="password"] {
		border-radius:6px 6px 6px 6px;
	}
	.login-button {
		width:75%;
	}
	#login-custom-title {
		display:none;
	}
}

